<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label "Set Song Title" %>
  <%= f.text_field :title %>
  <%= f.submit %>
  <br />
  <%= f.label "Artist Name" %>
  <%= f.text_field :artist_id %>
  <br/>
  <%= f.label "Genre" %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br/>
  <%= f.label "Notes" %>
  <%= f.text_field :genre_id %>
  <br/>
  <%= f.label "Notes" %>
  <%= f.text_field :genre_id %>
  

  <%= f.submit %>
<% end %>


